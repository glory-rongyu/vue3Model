{
  "name": "vue3Model",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev --port 9527",
    "test": "vue-tsc --noEmit && vite build --mode test && esno ./src/utils/build.ts",
    "build": "vue-tsc --noEmit && vite build --mode prod && esno ./src/utils/build.ts",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue src",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz",
    "commit-all": "git add . && cz",
    "commit-push": "git push -u origin main"
  },
  "dependencies": {
    "@vueuse/core": "^8.7.4",
    "axios": "^0.27.2",
    "element-plus": "^2.2.6",
    "mitt": "^3.0.0",
    "moment": "^2.29.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "v-code-diff": "^0.3.11",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "esno": "^0.16.3",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.2",
    "prettier": "2.7.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.0",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "src/**/*.{.js,.jsx,.ts,.tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
